{
  "name": "flea-asyncapi-generator-typescript-cdk-event-bridge",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/flea/asyncapi-generator-typescript-cdk-event-bridge/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/esbuild:esbuild",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/flea/asyncapi-generator-typescript-cdk-event-bridge",
        "main": "libs/flea/asyncapi-generator-typescript-cdk-event-bridge/src/index.ts",
        "tsConfig": "libs/flea/asyncapi-generator-typescript-cdk-event-bridge/tsconfig.lib.json",
        "assets": [
          "libs/flea/asyncapi-generator-typescript-cdk-event-bridge/*.md"
        ],
        "generatePackageJson": true,
        "format": ["cjs"]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/flea/asyncapi-generator-typescript-cdk-event-bridge/**/*.ts",
          "libs/flea/asyncapi-generator-typescript-cdk-event-bridge/package.json"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/flea/asyncapi-generator-typescript-cdk-event-bridge/jest.config.ts"
      }
    },
    "generate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "asyncapi generate fromTemplate libs/flea/asyncapi-generator-typescript-cdk-event-bridge/src/lib/test-fixtures/test-asyncapi.yaml libs/flea/asyncapi-generator-typescript-cdk-event-bridge/src -p server=localhost -o gen-out\n"
      }
    }
  },
  "tags": []
}
